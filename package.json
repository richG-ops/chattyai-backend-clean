{
  "name": "thechattyai-calendar-bot",
  "version": "1.0.1",
  "description": "TheChattyAI Calendar Bot for Voice-Based Appointment Booking",
  "main": "google-calendar-api.js",
  "scripts": {
    "start": "node src/index.js",
    "ensure:schema": "node scripts/ensure-schema.js",
    "start:legacy": "node google-calendar-api.js",
    "dev": "nodemon src/index.js",
    "test": "jest --coverage",
    "test:watch": "mocha tests/**/*.test.js --watch",
    "lint": "eslint .",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:status": "knex migrate:status",
    "smoke-test": "node tests/smoke-test.js",
    "smoke:staging": "node scripts/smoke-staging.js",
    "mint:jwt": "node scripts/mint-tenant-jwt.js",
    "deploy": "git push origin main",
    "prestart": "npm run smoke-test"
  },
  "dependencies": {
    "@sentry/node": "^7.120.3",
    "aws-sdk": "2.1692.0",
    "axios": "^1.10.0",
    "body-parser": "^1.20.2",
    "bottleneck": "^2.19.5",
    "bull": "4.16.5",
    "chai": "5.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "googleapis": "^128.0.0",
    "helmet": "^7.2.0",
    "ioredis": "5.6.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "luxon": "^3.7.1",
    "mocha": "11.7.1",
    "nodemailer": "^6.9.8",
    "open": "^10.1.2",
    "opossum": "9.0.0",
    "pg": "^8.16.3",
    "promise-retry": "^2.0.1",
    "redis": "^4.7.1",
    "sinon": "21.0.0",
    "stripe": "^18.3.0",
    "supertest": "7.1.3",
    "twilio": "^5.7.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "google-calendar",
    "vapi",
    "scheduling",
    "api"
  ],
  "author": "Richard Gallagher",
  "license": "MIT"
}
